{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@rxjs/*": [
        "app/rxjs/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@classes/*": [
        "app/classes/*"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@animations/*": [
        "app/animations/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
